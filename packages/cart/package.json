{
  "name": "@pkg/cart",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev:types": "tsc -w --noEmit",
    "dev:bundle": "tsup --watch",
    "dev": "npm run dev:types & npm run dev:bundle",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@pkg/analytics": "*",
    "@pkg/core": "*",
    "@pkg/design-system": "*",
    "@vercel/analytics": "^1.5.0",
    "zod": "^4.0.16"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "graphql": ">=16.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@pkg/config-jest": "*",
    "@pkg/config-tailwind": "*",
    "@pkg/domain": "*",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "next": "15.4.5",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwindcss": "^3.4.16",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  }
}